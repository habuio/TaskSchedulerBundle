{
    "name": "habu/task-scheduler-bundle",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^3.3.0-BETA1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Habu\\TaskSchedulerBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Habu\\TaskSchedulerBundle\\Tests\\": "Tests/"
        }
    },
    "license": "BSD 3-clause",
    "authors": [
        {
            "name": "Ruben Knol",
            "email": "c.minor6@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.3"
    }
}
